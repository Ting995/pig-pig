/* packages/ELSFK/index.wxss */
view{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}
.h40{
    height: 40rpx;
}
.reset{
    color: white;
    margin-top: 60rpx;
    background-color: rgb(243, 87, 47);
}
.fk-main{
    position: relative;
    margin: 0 auto;
    width: 642rpx;
    height: 840rpx;
    background: skyblue;
    border: 1px solid blue;
    overflow: hidden;
}
.fk-content{
    position: absolute;
    left: 0;
}
.fk-content-line{
    position: relative;
    height: 80rpx;
}
.fk-content-item{
    position: absolute;
    top: 0;
    width: 80rpx;
    height: 80rpx;
    border-radius: 10rpx;
}
.color-red{
    border: 1px solid red;
    background: red;
    box-shadow: 0 0 20rpx inset rgb(197, 0, 0);
}
.color-green{
    border: 1px solid green;
    background: rgb(0, 255, 0);
    box-shadow: 0 0 20rpx inset rgb(0, 121, 0);
}
.color-blue{
    border: 1px solid blue;
    background: blue;
    box-shadow: 0 0 20rpx inset rgb(2, 2, 190);
}
.color-yellow{
    border: 1px solid yellow;
    background: yellow;
    box-shadow: 0 0 20rpx inset rgb(148, 148, 0);
}
.color-tran{
    border: 1px solid transparent;
    background: transparent;
    box-shadow: 0 0 20rpx inset transparent;
}
.color-kong{
    background: transparent;
}